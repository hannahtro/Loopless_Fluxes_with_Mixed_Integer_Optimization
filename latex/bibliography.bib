	note = {Publisher: Proceedings of the National Academy of Sciences},
	file = {Full Text PDF:/home/hannah/Zotero/storage/5G2AMM7L/Beg et al. - 2007 - Intracellular crowding defines the mode and sequen.pdf:application/pdf},
}

