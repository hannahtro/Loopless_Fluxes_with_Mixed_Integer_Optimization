	langid = {english},
	keywords = {Steady-state flux cone, 52B05 Combinatorial properties of polytopes and polyhedra, 92C42 Systems biology Networks, Elementary flux modes, Face lattice, Metabolic networks},
	file = {Full Text PDF:/home/hannah/git/Zotero/storage/WNNCXMC8/Wieder et al. - 2023 - On the geometry of elementary flux modes.pdf:application/pdf},
}

