	note = {Publisher: John Wiley \& Sons, Ltd},
	keywords = {systems biology, genome analysis, genome-scale metabolic models, metabolic innovation},
	file = {Full Text PDF:/home/hannah/git/Zotero/storage/YWUS8DZS/Lu et al. - 2021 - Yeast metabolic innovations emerged via expanded m.pdf:application/pdf},
